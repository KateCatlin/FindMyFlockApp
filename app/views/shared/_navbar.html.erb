 <% if registration_signed_in? %>
    <% if current_user %>
      <ul class="container navbar-container">
        <li id="nav-left"> <%= link_to "Find my Flock", root_path %></li>
        <li class="underline" id="nav-right-padding"> <%= link_to "My Profile", user_path(current_user) %></li>
        <li class="underline" id="nav-right-padding"><%= link_to "Dashboard", dashboard_index_path %></li>
        <li class="underline-logout" id="nav-right"><%= link_to "Log out", destroy_registration_session_path,:method => :delete %></li>
      </ul>

      <% else %>
      <ul class="container navbar-container">
        <li id="nav-left"><%= link_to "Find my Flock", root_path %></li>
        <li class="underline-logout" id="nav-right"><%= link_to "Log out", destroy_registration_session_path,:method => :delete %></li>
      </ul>
      <% end %>
    <% else %>
      <ul class="container navbar-container">
        <li id="nav-left"><%= link_to "Find my Flock", root_path %></li>
        <li class="underline" id="nav-right-padding"><%= link_to "Log In", new_registration_session_path %></li>
        <li class="sign-up" id="nav-right"><%= link_to "Sign Up", new_registration_registration_path %></li>
      </ul>
  <% end %>
